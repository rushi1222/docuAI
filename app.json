{
  "expo": {
    "name": "Rushideep",
    "slug": "rushideep",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rushideeporg.rushideep"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rushideeporg.rushideep"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c1405dec-9280-41a1-8e8e-950d774ab4a3"
      }
    },
    "owner": "rushideep-org",
    "updates": {
      "url": "https://u.expo.dev/c1405dec-9280-41a1-8e8e-950d774ab4a3"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
